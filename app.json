{
  "expo": {
    "name": "rahdar",
    "slug": "rahdar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "androidStatusBar": {
      "translucent": false,
      "backgroundColor": "#FFFFFF",
      "barStyle": "dark-content"
    },
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F9EFEF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.iaki.rahdar",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.iaki.rahdar",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#F9EFEF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Precisamos acessar suas fotos para anexar comprovantes."
        }
      ],
      [
        "expo-location",
        {
          "locationPermission": "Precisamos da sua localização para encontrar lojas próximas."
        }
      ],
      "expo-font"
    ],
    "newArchEnabled": true,
    "runtimeVersion": "1.0.0",
    "extra": {
      "eas": {
        "projectId": "e4c3f372-e992-47dd-8c7c-5227b57468d1"
      }
    }
  }
}
