{
  "expo": {
    "name": "rahdar",
    "slug": "rahdar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.infojanio.icompras",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.infojanio.icompras",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Precisamos acessar suas fotos para anexar comprovantes."
        }
      ],
      [
        "expo-location",
        {
          "locationPermission": "Precisamos da sua localização para encontrar lojas próximas."
        }
      ],
      "expo-font"
    ],
    "newArchEnabled": true,
    "runtimeVersion": "1.0.0"
  }
}
